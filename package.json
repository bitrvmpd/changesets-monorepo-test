{
  "name": "@bitrvmpd/monorepo-changesets",
  "private": true,
  "devDependencies": {
    "@changesets/cli": "^2.27.7"
  },
  "engines": {
    "node": "20.15.1",
    "pnpm": ">=9.5.0"
  },
  "scripts": {
    "publish": "npm_config_registry=https://npm.pkg.github.com pnpm changeset publish",
    "version-snapshot": "npm_config_registry=https://npm.pkg.github.com pnpm changeset version --snapshot",
    "publish-snapshot": "npm_config_registry=https://npm.pkg.github.com pnpm changeset publish --no-git-tag --tag"
  },
  "packageManager": "pnpm@9.5.0"
}
